 <%- include('../templates/header.ejs')   %>
  <div class="tt">
    <div>Admin Dashboard  </div>
     <a href="/view_activities" class="btn btn-primary">View All Activities</a>
    <a href="/logout" class="btn btn-warning">Logout</a>
  </div>
   
  <table class="table bordered table-striped t_data">
    <thead>
      <tr>
        <th>S.No</th>
        <th>User Name</th>
        <th>Task Heading</th>
        <th>Description</th>
        <th></th>
        <th></th>
        
      </tr>
    </thead>
    <tbody>
      <% tasks.forEach((user,k) => { %>
        <tr>
          <td><%= k+1 %></td>
          <td><%= user.email %></td>
          <td><%= user.task_heading %></td>
          <td><%= user.description %></td>
          
        </tr>
      <% }); %>
    </tbody>
  </table>
<%- include('../templates/footer.ejs')   %>